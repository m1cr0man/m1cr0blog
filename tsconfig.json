{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"outDir": "dist",
		"strict": true,
		"sourceMap": true,
		"removeComments": true,
		"noUnusedLocals": true,
		"noImplicitAny": false,
		"noImplicitReturns": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": [
		"src/**/*.ts"
	],
	"exclude": [
		"node_modules"
	]
}
